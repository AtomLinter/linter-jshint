{
  "name": "linter-jshint",
  "main": "./lib/main",
  "version": "2.1.0",
  "description": "Linter plugin for JavaScript, using jshint",
  "repository": "https://github.com/AtomLinter/linter-jshint.git",
  "license": "MIT",
  "package-deps": [
    "linter"
  ],
  "dependencies": {
    "atom-linter": "^4.6.1",
    "atom-package-deps": "^4.0.1",
    "jshint": "^2.9.1",
    "jshint-json": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^2.7.0",
    "babel-eslint": "^6.0.2",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-react": "^4.3.0",
    "eslint-plugin-jsx-a11y": "^1.0.1"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "no-console": 0,
      "semi": [
        2,
        "never"
      ],
      "no-nested-ternary": 0
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
