{
  "name": "linter-jshint",
  "main": "./lib/main",
  "version": "1.3.1",
  "description": "Linter plugin for JavaScript, using jshint",
  "repository": "https://github.com/AtomLinter/linter-jshint.git",
  "license": "MIT",
  "dependencies": {
    "atom-linter": "^4.5.0",
    "jshint": "^2.9.1",
    "jshint-json": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "babel-eslint": "^4.1.8",
    "eslint-config-airbnb": "^5.0.1"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "no-console": 0,
      "semi": [2, "never"],
      "no-nested-ternary": 0
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
